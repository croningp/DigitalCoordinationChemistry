# 27 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/Grad.incl"
define list<gradient> EA=<EA>


# 31 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/Grad.incl"
;$Id: Grad2.incl,v 1.14 2012/01/31 17:49:21 ber Exp $
# 23 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf" 2


# 26 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
"d11=30m"
"d12=20u"
"d13=4u"


"in0=inf1"
# 1 "mc_line 34 file C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf exp. def. part of mc cmd. before ze"
; dimension 2D; AQ_mode  (F1) QF
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ST1CNT
"ST1CNT = trunc(td1 / 1)"
"MCREST = d11 - d11"
"MCWRK = 0.500000*d11"

    dccorr
# 34 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
# 34 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
1 ze
# 1 "mc_line 34 file C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf exp. def. of mc cmd. after ze"
LBLAV, MCWRK
      "d0=d0orig + t1loop * in0 "
# 35 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
# 1 "mc_line 35 file C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf exp. start label for mc cmd."
2 MCWRK 
LBLST1, MCWRK
  MCREST
# 36 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"

3 d12 pl10:f1
  p17 ph4
  p17*2 ph5
  d1 pl1:f1

  p1 ph1
  d0
  50u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0
  p16:gp1
  d16
  p1 ph2
  d13
  p16:gp2
  d16
  p1 ph3
  d13
  p16:gp3
  d16
  4u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7
  go=2 ph31
# 1 "mc_line 57 file C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf exp. mc cmd. in line"
  MCWRK  wr #0 if #0 zd 

  "t1loop+=1"
      "d0=d0orig + t1loop * in0 "
  lo to LBLST1 times ST1CNT
  MCWRK 
  "t1loop=0"
  MCWRK rf #0
  lo to LBLAV times tdav
# 58 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
exit
# 61 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
ph1=0 2
ph2=0 
ph3=0 
ph4=0 
ph5=1 
ph31=0 2 


;pl1 : f1 channel - power level for pulse (default)
;pl10: f1 channel - power level for TOCSY-spinlock
;p1 : f1 channel -  90 degree high power pulse
;p16: homospoil/gradient pulse
;p17: f1 channel - trim pulse                        [2.5 msec]
;d0 : incremented delay (2D)                         [3 usec]
;d1 : relaxation delay; 1-5 * T1
;d12: delay for power switching                      [20 usec]
;d11: delay for disk I/O                             [30 msec]
;d13: short delay                                    [4 usec]
;d16: delay for homospoil/gradient recovery
;inf1: 1/SW = 2 * DW
;in0: 1/(1 * SW) = 2 * DW
;nd0: 1
;ns: 1 * n
;ds: 16
;td1: number of experiments
;FnMODE: QF
# 88 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
;use gradient ratio:	gp 1 : gp 2 : gp 3
;			  16 :   12 :   40    for dq filter
;			   4 :   12 :   40    for tq filter

;for z-only gradients:
;gpz1: 16% for DQF, 4% for TQF
;gpz2: 12%
;gpz3: 40%

;use gradient files:   
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100
;gpnam3: SMSQ10.100
# 104 "C:/Bruker/TopSpin3.6.3/exp/stan/nmr/lists/pp/cosygpmfppqf"
